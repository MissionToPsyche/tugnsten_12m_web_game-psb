    <Style src="project://database/Assets/Resources/UI/USS/Stylesheet.uss?fileID=7433441132597879392&amp;guid=30fb6192f07eb614bb3c2983da32c6ec&amp;type=3#Stylesheet" />
    <ui:VisualElement name="info-content" style="flex-grow: 1; padding-top: 1%; padding-right: 1%; padding-bottom: 1%; padding-left: 1%;">
        <ui:Label tabindex="-1" text="Psyche’s multispectral imager&#10;consists of a pair of identical &#10;cameras equipped with filters&#10;and telescopic lenses to &#10;photograph the surface of the &#10;asteroid in different wavelengths&#10;of light. The cameras can take&#10;pictures in the part of the &#10;spectrum visible to the human eye,&#10;as well as in near-infrared wavelengths of light beyond what humans can see. The images will help the science team learn about the mineral composition of Psyche; map the distribution of craters, valleys, cliffs, and other geologic features; and enable the creation of 3D topographic maps – all of which will provide clues to Psyche’s history. " parse-escape-sequences="true" display-tooltip-when-elided="true" name="text-content" class="info-text-content" style="height: 35%;" />
        <ui:VisualElement name="image-1" class="info-image" style="flex-grow: 1; visibility: visible; background-image: url(&apos;project://database/Assets/imager.png?fileID=2800000&amp;guid=952e587be4b5912459932e03113ed1b4&amp;type=3#imager&apos;); align-self: center; position: absolute; top: 1%; left: 51%; height: 280px;" />
        <ui:VisualElement name="image-2" class="info-image" style="flex-grow: 1; visibility: visible; background-image: url(&apos;project://database/Assets/imager_light.png?fileID=2800000&amp;guid=aed20079ff7963c4eaaa8eeb87c08843&amp;type=3#imager_light&apos;); align-self: center; position: relative; width: 850px; height: 550px;" />
        <ui:VisualElement name="image-3" class="info-image" style="flex-grow: 1; visibility: visible; background-image: url(&apos;project://database/Assets/imagersOnCraft.png?fileID=2800000&amp;guid=8b09c124110a8924dab35eb9bb71a4aa&amp;type=3#imagersOnCraft&apos;); align-self: center; width: 850px; height: 550px;" />
